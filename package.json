{
  "name": "app",
  "private": true,
  "devDependencies": {
    "esbuild": "^0.25.10"
  },
  "scripts": {
    "build": "esbuild app/javascript/*.* --bundle --sourcemap --format=esm --outdir=app/assets/builds --public-path=/assets --loader:.js=jsx",
    "build:css": "TAILWIND_DISABLE_TOUCH=true npx @tailwindcss/cli -i ./app/assets/stylesheets/application.tailwind.css -o ./app/assets/builds/application.css --minify --no-autoprefixer"
  },
  "dependencies": {
    "@tailwindcss/cli": "^4.1.14",
    "@tiptap/extension-color": "^3.8.0",
    "@tiptap/extension-highlight": "^3.8.0",
    "@tiptap/extension-image": "^3.8.0",
    "@tiptap/extension-link": "^3.8.0",
    "@tiptap/extension-table": "^3.8.0",
    "@tiptap/extension-table-cell": "^3.8.0",
    "@tiptap/extension-table-header": "^3.8.0",
    "@tiptap/extension-table-row": "^3.8.0",
    "@tiptap/extension-text-align": "^3.8.0",
    "@tiptap/extension-text-style": "^3.8.0",
    "@tiptap/extension-underline": "^3.8.0",
    "@tiptap/pm": "^3.8.0",
    "@tiptap/react": "^3.8.0",
    "@tiptap/starter-kit": "^3.8.0",
    "force-graph": "^1.51.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-force-graph-2d": "^1.29.0",
    "tailwindcss": "^4.1.14"
  }
}
